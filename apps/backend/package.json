{
  "name": "db-manager-server",
  "version": "1.0.0",
  "description": "Api Express to manage incoming HTTP requests",
  "type": "commonjs",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "tsc",
    "dev": "nodemon",
    "test": "dotenv-cli -e .env -- tsx src/index.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.{ts,json,md}\""
  },
  "keywords": [
    "api",
    "http",
    "express",
    "stexcore",
    "server"
  ],
  "license": "ISC",
  "dependencies": {
    "@stexcore/api-engine": "^2.0.0",
    "@stexcore/http-status": "^1.0.1",
    "express": "^5.1.0",
    "mariadb": "^3.4.5",
    "morgan": "^1.10.1",
    "mysql2": "^3.15.0",
    "oracledb": "^6.9.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "tedious": "^18.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0"
  }
}
